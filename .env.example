# =============================================================================
# Visual Agent Builder - Environment Variables
# =============================================================================
# Copy this file to .env and fill in your API keys.

# === REQUIRED ===

# Anthropic API key for workflow execution (orchestrator bridge)
ANTHROPIC_API_KEY=sk-ant-your-key-here

# Base directory for component inventory scanning
INVENTORY_ROOT=/path/to/your/component-library

# === OPTIONAL ===

# Server port (default: 3001)
PORT=3001

# Node environment (development | production)
NODE_ENV=development

# Google AI API key (for Supervisor agent - Gemini)
GOOGLE_API_KEY=your-google-api-key-here

# === MULTI-WORKSPACE FAILOVER (Advanced) ===

# Builder agent keys (primary + backup workspace)
BUILDER_KEY_PRIMARY=sk-ant-your-builder-primary-key
BUILDER_KEY_BACKUP=sk-ant-your-builder-backup-key

# Architect agent keys (primary + backup workspace)
ARCHITECT_KEY_PRIMARY=sk-ant-your-architect-primary-key
ARCHITECT_KEY_BACKUP=sk-ant-your-architect-backup-key

# === CORS ===

# Comma-separated allowed origins (default: http://localhost:5173,http://localhost:3000)
CORS_ORIGINS=http://localhost:5173,http://localhost:3000
